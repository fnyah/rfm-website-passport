    <script>
      // ajax call
      const getPhotos = () => {
        $.ajax({
          url: "http://localhost:3000/admin/home/files",
          type: "GET",
          success: function (data) {
            console.log("working");
            for (let i = 0; i < data.length; i++) {
              // create a new slide for each photo and iterate the class name by 1
              let newSlide = document.createElement("div");
              newSlide.className = `slide-${i + 1} w-slide`;
              // set background image of slide
              newSlide.style.backgroundImage = `url(http://localhost:3000/admin/home/image/${data[i].filename})`;
              // create a new image for each photo and iterate the class name by 1
              // append the image to the slide
              let newTitle = document.createElement("h1");
              newTitle.innerText = "AAAAAAAAAAAAAA"
              newSlide.appendChild(newTitle);
              console.log("appended image");
              // append the slide to the slider container
              document.getElementById("masterSlider").appendChild(newSlide);
            }
          },
        });
      };

      // execute function whne page loads
      $(document).ready(function () {
        getPhotos();
      });
    </script>

